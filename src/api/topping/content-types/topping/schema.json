{
  "kind": "collectionType",
  "collectionName": "toppings",
  "info": {
    "singularName": "topping",
    "pluralName": "toppings",
    "displayName": "Toppings",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "cakes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cake.cake",
      "inversedBy": "toppings"
    },
    "images": {
      "type": "component",
      "repeatable": true,
      "component": "images.topping-image",
      "required": true,
      "min": 1,
      "max": 2
    },
    "description": {
      "type": "text",
      "required": true
    },
    "uniqe": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "price_double": {
      "type": "string",
      "required": true,
      "default": "100 / 100 грам"
    }
  }
}
